<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.prac.s1.bankbook.BankBookDAO">
	<select id="list" resultType="com.prac.s1.bankbook.BankBookDTO">
		SELECT * FROM BANKBOOK
	</select>
	<select id="detail" parameterType="com.prac.s1.bankbook.BankBookDTO" resultType="com.prac.s1.bankbook.BankBookDTO">
	SELECT *FROM BANKBOOK WHERE BOOKNUMBER=#{bookNumber}
	</select>
	<delete id="delete" parameterType="com.prac.s1.bankbook.BankBookDTO">
	DELETE BANKBOOK WHERE BOOKNUMBER=#{bookNumber}
	</delete>
		<insert id="add" parameterType="com.prac.s1.bankbook.BankBookDTO">
		INSERT INTO BANKBOOK(BOOKNUMBER,BOOKNAME,BOOKCONTENTS,BOOKRATE,BOOKSALE)
		VALUES(BANKBOOK_SEQ.NEXTVAL,#{bookName},#{bookContents},#{bookRate},#{bookSale})
	</insert>



</mapper> 